{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": "logs:CreateLogGroup",
      "Resource": "arn:aws:logs:us-east-2:$$$ACCOUNT_ID$$$:*"
    },
    {
      "Effect": "Allow",
      "Action": [
        "logs:CreateLogStream",
        "logs:PutLogEvents"
      ],
      "Resource": [
        "arn:aws:logs:us-east-2:$$$ACCOUNT_ID$$$:log-group:/aws/lambda/$$$LAMBDA_NAME$$$:*"
      ]
    }
  ]
}
